<% content_for :title_extra do %>
 | Archive
<% end %>

<% @comics.each do |comic| %>
  <%= render :partial => 'minimal_comic', :locals => {:comic => comic} %>
<% end %>
<% if user_is_author? %>
  <div class="new_comic">
    <%= link_to('+ New Comic', new_comic_path) %>
  </div>
<% else %>
  <div class="clear" > </div>
<% end %>
